  "tokenColors": [
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Storage modifiers (static, final, abstract, etc.)",
      "scope": "storage.modifier",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage types (class, function, int, var, etc.)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#78DCE8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type declarations and references",
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#78DCE8"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "entity.name.variable", "support.variable"],
      "settings": {
        "foreground": "#FCFCFA"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "constant",
        "entity.name.constant",
        "support.constant",
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#F59762",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Reserved language variables (this, super, self, etc.)",
      "scope": "variable.language",
      "settings": {
        "foreground": "#F59762"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#727072"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Markup headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#FFD866",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "name": "Link strings",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Markup quoted text",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#727072",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup bold text",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic text",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup strikethrough text",
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Informational messages",
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "name": "Warnings",
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "name": "Errors",
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "Debug information",
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ],
  "semanticHighlighting": true
}
