  "tokenColors": [
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Storage modifiers (static, final, abstract, etc.)",
      "scope": "storage.modifier",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage types (class, function, int, var, etc.)",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#78DCE8"
      }
    },
    {
      "name": "Type declarations and references",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#78DCE8"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "entity.name.variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#FCFCFA"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "constant",
        "entity.name.constant",
        "support.constant",
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F59762"
      }
    },
    {
      "name": "Reserved language variables (this, super, self, etc.)",
      "scope": "variable.language",
      "settings": {
        "foreground": "#F59762"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#727072"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Markup headings",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD866"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#FCFCFA"
      }
    },
    {
      "name": "Link strings",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Markup quoted text",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#727072"
      }
    },
    {
      "name": "Markup bold text",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic text",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup strikethrough text",
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup inlines (inline code, etc.)",
      "scope": "markup.inline",
      "settings": {
        "foreground": "#F59762"
      }
    },
    {
      "name": "Informational messages",
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "name": "Warnings",
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "name": "Errors",
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "Debug information",
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ],
  "semanticHighlighting": true
}