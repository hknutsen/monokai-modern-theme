{
  "$schema": "vscode://schemas/color-theme",
  "name": "Monokai Modern",
  "type": "dark",
  "colors": {
    "activityBar.background": "#181818",
    "activityBar.foreground": "#fcfcfa",
    "activityBar.border": "#2b2b2b",

    "editor.background": "#221f22",
    "editor.foreground": "#fcfcfa",
    "editorGroup.border": "#2b2b2b",
    "editorGroupHeader.tabsBackground": "#181818",
    "editorBracketHighlight.foreground1": "#ff6188",
    "editorBracketHighlight.foreground2": "#f59762",
    "editorBracketHighlight.foreground3": "#ffd866",
    "editorBracketHighlight.foreground4": "#a9dc76",
    "editorBracketHighlight.foreground5": "#78dce8",
    "editorBracketHighlight.foreground6": "#ab9df2",

    "menu.background": "#221f22",

    "panel.background": "#181818",
    "panel.border": "#2b2b2b",

    "sideBar.background": "#181818",
    "sideBar.foreground": "#fcfcfa",
    "sideBar.border": "#2b2b2b",
    "sideBarSectionHeader.background": "#181818",
    "sideBarSectionHeader.border": "#2b2b2b",

    "statusBar.background": "#181818",
    "statusBar.foreground": "#fcfcfa",
    "statusBar.border": "#2b2b2b",

    "tab.activeBackground": "#221f22",
    "tab.inactiveBackground": "#181818",
    "tab.border": "#2b2b2b",

    "terminal.foreground": "#fcfcfa",
    "terminal.ansiBlack": "#727072",
    "terminal.ansiBlue": "#fc9867",
    "terminal.ansiCyan": "#78dce8",
    "terminal.ansiGreen": "#a9dc76",
    "terminal.ansiMagenta": "#ab9df2",
    "terminal.ansiRed": "#ff6188",
    "terminal.ansiWhite": "#fcfcfa",
    "terminal.ansiYellow": "#ffd866",
    "terminal.ansiBrightBlack": "#727072",
    "terminal.ansiBrightBlue": "#fc9867",
    "terminal.ansiBrightCyan": "#78dce8",
    "terminal.ansiBrightGreen": "#a9dc76",
    "terminal.ansiBrightMagenta": "#ab9df2",
    "terminal.ansiBrightRed": "#ff6188",
    "terminal.ansiBrightWhite": "#fcfcfa",
    "terminal.ansiBrightYellow": "#ffd866",

    "titleBar.activeBackground": "#181818",
    "titleBar.inactiveBackground": "#221f22",
    "titleBar.activeForeground": "#fcfcfa",
    "titleBar.border": "#2b2b2b"
  },
  "tokenColors": [
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Storage modifiers (static, final, abstract, etc.)",
      "scope": "storage.modifier",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage types (class, function, int, var, etc.)",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#78DCE8"
      }
    },
    {
      "name": "Type declarations and references",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#78DCE8"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "entity.name.variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#FCFCFA"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "constant",
        "entity.name.constant",
        "support.constant",
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F59762"
      }
    },
    {
      "name": "Reserved language variables (this, super, self, etc.)",
      "scope": "variable.language",
      "settings": {
        "foreground": "#F59762"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#727072"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Markup headings",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD866"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#FCFCFA"
      }
    },
    {
      "name": "Link strings",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "name": "Markup quoted text",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#727072"
      }
    },
    {
      "name": "Markup bold text",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic text",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup strikethrough text",
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup inlines (inline code, etc.)",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#F59762"
      }
    }
  ],
  "semanticHighlighting": true
}
